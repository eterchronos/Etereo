<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="471.0" prefWidth="309.0" style="-fx-background-color: #21252b;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OccupationController">
   <children>
      <TableView fx:id="table" layoutX="14.0" layoutY="15.0" onMouseClicked="#tableMouseClick" prefHeight="371.0" prefWidth="278.0" style="-fx-background-radius: 5;">
        <columns>
          <TableColumn fx:id="tableOccupation" prefWidth="75.0" text="Cargo" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnInsert" layoutX="15.0" layoutY="429.0" mnemonicParsing="false" onAction="#InsertItemTableButton" prefHeight="26.0" prefWidth="133.0" style="-fx-background-radius: 5;" text="Adicionar" />
      <Button fx:id="btnDelete" layoutX="159.0" layoutY="429.0" mnemonicParsing="false" onAction="#DeleteItemTableButton" prefHeight="26.0" prefWidth="133.0" style="-fx-background-radius: 5;" text="Remover" />
      <TextField fx:id="jtfOccupation" alignment="CENTER" layoutX="15.0" layoutY="395.0" prefHeight="26.0" prefWidth="278.0" promptText="Insira aqui um novo cargo" style="-fx-background-radius: 10;" />
   </children>
</AnchorPane>
